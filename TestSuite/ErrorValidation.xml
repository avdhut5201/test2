<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">


<suite parallel="tests" name="Suite">

	<groups>
		<run>
			<include name="ErrorHandling" />
		</run>
	</groups>

	<listeners>
		<listener class-name="Avdhut.SeleniumFramework.TestComponent.Listner"></listener>
	</listeners>
	<test thread-count="5" name=" Submit Order Test">
		<classes>
			<class name="Avdhut.SeleniumFramework.SubmitOrderTest" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="Error Validation Test">
		<classes>
			<class name="Avdhut.SeleniumFramework.ErrorValidation" />
		</classes>
	</test> <!-- Test -->


</suite> <!-- Suite -->